{
  "name": "ember-promise-modals",
  "version": "0.2.1",
  "description": "The default blueprint for ember-cli addons.",
  "keywords": [
    "ember-addon"
  ],
  "repository": "https://github.com/simplabs/ember-promise-modals",
  "license": "MIT",
  "author": "simplabs GmbH",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "style": "addon/styles/ember-promise-modals.css",
  "scripts": {
    "build": "ember build",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:hbs": "ember-template-lint .",
    "lint:js:fix": "eslint . --fix",
    "lint:js": "eslint . --cache",
    "lint:md:fix": "prettier --write \"**/*.md\"",
    "lint:md": "prettier --check \"**/*.md\"",
    "lint:yaml:fix": "prettier --write \"**/*.{yml,yaml}\"",
    "lint:yaml": "prettier --check \"**/*.{yml,yaml}\"",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel 'lint:!(fix)'",
    "start": "ember serve",
    "test:all": "ember try:each",
    "test": "npm-run-all lint test:*"
  },
  "dependencies": {
    "broccoli-funnel": "^3.0.3",
    "broccoli-merge-trees": "^4.2.0",
    "broccoli-postcss": "^5.1.0",
    "ember-auto-import": "^1.10.1",
    "ember-cli-babel": "^7.23.1",
    "ember-cli-htmlbars": "^5.2.0",
    "focus-trap": "^6.3.0",
    "postcss-preset-env": "^6.7.0"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.1.4",
    "broccoli-asset-rev": "^3.0.0",
    "ember-cli": "~3.24.0",
    "ember-cli-dependency-checker": "^3.1.0",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-uglify": "^3.0.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.2",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-page-title": "^6.0.3",
    "ember-qunit": "^5.1.1",
    "ember-resolver": "^8.0.2",
    "ember-source": "~3.24.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^2.16.0",
    "ember-try": "^1.4.0",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-ember": "^10.1.1",
    "eslint-plugin-import-helpers": "^1.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.2.1",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "qunit": "^2.13.0",
    "qunit-dom": "^1.6.0"
  },
  "engines": {
    "node": "10.* || 12.* || >= 14.*"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
